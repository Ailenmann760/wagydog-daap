# External APIs (optional - will use mocks if not provided)
GECKOTERMINAL_API_KEY=
MORALIS_API_KEY=
ALCHEMY_API_KEY=

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Cache
CACHE_TTL=300
