# Backend Environment Variables

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/wagydog_analytics?schema=public"

# Server
PORT=4000
NODE_ENV=development

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Admin
ADMIN_DEFAULT_EMAIL=admin@wagydog.com
